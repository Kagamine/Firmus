<div class="wrap-cont">
    <form action="/mobile/login" method="post">
        <input type="hidden" name="_csrf" value="<%= csrf %>" />
        <div class="textbox-outer">
            <input type="text" name="username" placeholder="用户名" class="textbox">
        </div>
        <div class="textbox-outer">
            <input type="password" name="password" placeholder="密码" class="textbox">
        </div>
        <a href="javascript: $('form').submit();">
            <div class="btn">登录</div>
        </a>
    </form>
</div>