<div class="wrap-cont">
    <div id="lstMyOrders">

    </div>
</div>

<script>
    var p = 0;
    var lock = false;
    function Load()
    {
        if (lock) return;
        lock = true;
        $.get('/mobile/myorders', { p: p }, function (data) {
            $('#lstMyOrders').append(data);
            p++;
            lock = false;
        });
    }
    Load();
    $(window).scroll(function () {
        var totalheight = parseFloat($(window).height()) + parseFloat($(window).scrollTop());
        if ($(document).height() <= totalheight) {
            Load();
        }
    });
</script>