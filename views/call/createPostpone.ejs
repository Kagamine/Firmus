<form class="block" method="post">
    <input type="hidden" name="_csrf" value="<%= csrf %>" />
    <table class="detail-table">
        <tr>
            <td class="row-title">变更起始时间</td>
            <td><input type="text" name="begin" class="textbox w-3 datetime" /></td>
        </tr>
        <tr>
            <td class="row-title">变更结束时间</td>
            <td><input type="text" name="end" class="textbox w-3 datetime" /></td>
        </tr>
        <tr>
            <td class="row-title">地址</td>
            <td>
                <span id="showOrderAddress"></span>
                <a href="javascript:createOrderSelectAddress();" id="btnSetAddress">点击设置地址</a>
                <input type="hidden" name="address" id="orderAddress" />
            </td>
        </tr>
        <tr>
            <td class="row-title">备注</td>
            <td>
                <textarea name="hint" class="ckeditor"></textarea>
                <script src="/scripts/ckeditor/ckeditor.js"></script>
            </td>
        </tr>
    </table>
    <p><input type="submit" value="提交变更" class="button blue" /></p>
</form>