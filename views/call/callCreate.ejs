<h2><%= title %></h2>
<form method="post" class="block">
    <input type="hidden" name="_csrf" value="<%= csrf %>" />
    <h3>来电信息录入</h3>
    <table class="detail-table">
        <tr>
            <td class="row-title">业务员</td>
            <td><input type="text" class="textbox w-3" name="user" /></td>
        </tr>
        <tr>
            <td class="row-title">订单号</td>
            <td><input type="text" class="textbox w-3" name="order" /></td>
        </tr>
        <tr>
            <td class="row-title">是否需要回访</td>
            <td>
               <input type="checkbox" name="needFeedback" id="chCallNeedFeedback" />
            </td>
        </tr>
        <tr style="display: none;" id="trCallIsFeedbacked">
            <td class="row-title">是否已经回访</td>
            <td>
                <input type="checkbox" class="textbox w-3" name="isFeedbacked" id="chCallIsFeedbacked" />
            </td>
        </tr>
        <tr style="display: none;" id="trCallFeedbackResult">
            <td class="row-title">回访结果</td>
            <td><input type="text" class="textbox w-3" name="feedbackResult" /></td>
        </tr>
        <tr>
            <td class="row-title">备注</td>
            <td><input type="text" class="textbox w-3" name="hint" /></td>
        </tr>
        <tr>
            <td class="row-title">内容</td>
            <td><input type="text" class="textbox w-3" name="content" /></td>
        </tr>
        <tr>
            <td class="row-title">热线类型编码</td>
            <td><input type="text" class="textbox w-3" name="type" />例如：1</td>
        </tr>
    </table>
    <p><input type="submit" value="增加来电信息" class="button blue" /></p>
</form>