<style>
    td, th {
        padding: 3px;
    }
    table {
        font-size: 9px;
        text-align: left;
    }
</style>
<button onClick="window.print()">打印</button>
<% for (var x in report2) { %>
<p><%= x %></p>
<table class="detail-table">
    <thead>
    <tr>
        <td class="row-title">奶品品相</td>
        <td>数量(瓶)</td>
    </tr>
    </thead>
    <tbody>
    <% for (var y in report2[x]) { %>
    <tr>
        <td class="row-title"><%= y %></td>
        <td><%= report2[x][y] %></td>
    </tr>
    <% } %>
    </tbody>
</table>
<% } %>
<br />
<table class="table">
    <thead class="tl">
    <tr>
        <th>订单号</th>
        <th>奶站</th>
        <th>配送人员</th>
        <th>客户姓名</th>
        <th>奶品</th>
        <th>数量</th>
        <th>地址</th>
        <th>联系电话</th>
        <th></th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    <% report.forEach(x => { %>
    <tr>
        <td><%= x.number %></td>
        <td><%= x.milkStation %></td>
        <td><%= x.distributor %></td>
        <td><%= x.customer %></td>
        <td><%= x.milkType %></td>
        <td><%= x.count %></td>
        <td><%= x.address %></td>
        <td><%= x.tel %></td>
        <td><%= x.storey %></td>
        <td><%= x.hint %></td>
    </tr>
    <% }) %>
    </tbody>
</table>