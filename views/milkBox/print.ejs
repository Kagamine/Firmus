<h2>奶箱装/拆/调换通知单</h2>
<style>
    table {
        border-collapse:collapse;
    }
    td {
        border: 1px solid #000;
    }
</style>
<table style="width: 100%">
    <tr>
        <td>通知单号</td>
        <td><%= store._id %></td>
        <td>执行仓库</td>
        <td><%= store.department.title %></td>
    </tr>
    <tr>
        <td>属性</td>
        <td><%= store.operateType %></td>
        <td>追踪状态</td>
        <td><%= store.milkBox %></td>
    </tr>
    <tr>
        <td>生成日期</td>
        <td><%= moment(store.time).format('YYYY-MM-DD hh:mm:ss') %></td>
        <td>数量</td>
        <td><%= store.count %></td>
    </tr>
    <tr>
        <td>关联订单</td>
        <td><%= store.order.number %></td>
        <td>地址</td>
        <td><%= store.order.address.city %><%= store.order.address.district %><%= store.order.address.address %></td>
    </tr>
    <tr>
        <td>联系人</td>
        <td><%= store.order.address.name %></td>
        <td>联系电话</td>
        <td><%= store.order.address.phone %></td>
    </tr>
</table>
<script>
    window.print();
</script>