<style>
    table {
        border-collapse:collapse;
        width: 100%;
    }
    td {
        border: 1px solid #000;
    }
</style>
<h4>装箱日报 <%= moment(req.query.time).format('YYYY-MM-DD') %></h4>
<% if (req.query.time) { %>
<% for (var x in data) { %>
<h5><%= x %></h5>
<table>
    <tr>
        <td>订单号</td>
        <td>类型</td>
        <td>状态</td>
        <td>地址</td>
        <td>联系人</td>
        <td>电话</td>
    </tr>
    <% data[x].forEach(x => { %>
    <tr>
        <td><%= x.order.number %></td>
        <td><%= x.operateType %></td>
        <td><%= x.milkBox %></td>
        <td><%= x.order.address.address %></td>
        <td><%= x.order.address.name %></td>
        <td><%= x.order.address.phone %></td>
    </tr>
    <% }) %>
</table>
<% } %>
<% } %>
<script>
    window.print();
</script>