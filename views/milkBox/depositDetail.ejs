<% include depositMenu %>
<form class="block" method="post">
    <h3><%= title %></h3>
    <table class="detail-table">
        <tr>
            <td class="row-title">
                订单号
            </td>
            <td>
                <%=deposit.number%>
            </td>
        </tr>
        <tr>
            <td class="row-title">地址</td>
            <td>
               <%=deposit.address.city%> <%=deposit.address.district%><%=deposit.address.address%>
            </td>
        </tr>

        <tr>
            <td class="row-title">是否需要拆箱</td>
            <td>
                <% if(deposit.giveBackFlag){
                %>
                <span>是</span>
                <%
                }else{
                %>
                <span>否</span>
                <%
                }%>
            </td>
        </tr>

        <%
        if(deposit.giveBackFlag){
        %>
        <tr id="trDepositGiveBackDone">
            <td class="row-title">是否已经拆箱</td>
            <td>
                <% if(deposit.giveBackDone){
                %>
                <span>是</span>
                <%
                }else{
                %>
                <span>否</span>
                <%
                }%>
            </td>
        </tr>
        <%
        if(deposit.giveBackDone){
        %>
        <tr id="trDepositTime">
            <td class="row-title">拆箱时间</td>
            <td><%= moment(deposit.giveBackTime).format('YYYY-MM-DD') %></td>
        </tr>
        <%
        }}
        %>

        <tr>
            <td class="row-title">是否需要装箱</td>
            <td>
                <% if(deposit.boxedFlag){
                %>
                <span>是</span>
                <%
                }else{
                %>
                <span>否</span>
                <%
                }%>
            </td>
        </tr>

        <%
        if(deposit.boxedFlag){
        %>
        <tr id="trDepositBoxedDone">
            <td class="row-title">是否已经装箱</td>
            <td>
                <% if(deposit.boxedDone){
                %>
                <span>是</span>
                <%
                }else{
                %>
                <span>否</span>
                <%
                }%>
            </td>
        </tr>
        <%
        if(deposit.boxedDone){
        %>
        <tr id="trDepositBoxedTime">
            <td class="row-title">装箱时间</td>
            <td><%= moment(deposit.boxedTime).format('YYYY-MM-DD') %></td>
        </tr>
        <%
        }}
        %>
    </table>
</form>