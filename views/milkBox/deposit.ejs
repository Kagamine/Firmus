<h2>
    <%=title%>
    <% if (permission.deposit.modify) { %>
    <a class="add" href="/milkBox/createDeposit">押金单</a>
    <% } %>
</h2>
<form class="search">
    检索：
    <select name="giveBackFlag"  id="slDepositSearchGiveBackFlag">
        <option value="">是否拆箱</option>
        <option value="0">否</option>
        <option value="1">是</option>
    </select>&nbsp;
    <input type="text" class="textbox w-0-8 datetime" placeholder="起始日期" name="begin" value="<%= req.query.begin %>" />&nbsp;
    <input type="text" class="textbox w-0-8 datetime" placeholder="截止日期" name="end" value="<%= req.query.end %>" />&nbsp;
    <input type="submit" value="过滤" class="button green" style="margin: 0" value="过滤" />
</form>

<table class="table">
    <thead>
    <tr class="tl">
        <th>单号</th>
        <th>地址</th>
        <th>时间</th>
        <th>操作</th>
    </tr>
    </thead>
    <tbody>
    </tbody>
</table>


